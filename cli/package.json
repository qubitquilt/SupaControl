{
  "name": "supacontrol-installer",
  "version": "0.1.0",
  "description": "Interactive installation wizard for SupaControl",
  "type": "module",
  "bin": {
    "supacontrol-install": "./dist/cli.js"
  },
  "scripts": {
    "start": "tsx src/cli.tsx",
    "dev": "node --import tsx --watch src/cli.tsx",
    "build": "esbuild src/cli.tsx --bundle --platform=node --target=node18.19 --outfile=dist/cli.js --external:react --external:ink --external:ink-* --banner:js='#!/usr/bin/env node'",
    "prepublishOnly": "npm run build",
    "postinstall": "patch-package",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "supacontrol",
    "supabase",
    "kubernetes",
    "installer",
    "cli"
  ],
  "author": "QubitQuilt",
  "license": "MIT",
  "dependencies": {
    "boxen": "^5.1.2",
    "chalk": "^5.3.0",
    "execa": "^8.0.1",
    "ink": "^4.4.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^5.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^5.0.1",
    "nanoid": "^5.0.4",
    "ora": "^7.0.1",
    "react": "^18.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-test-renderer": "^19.1.0",
    "@vitest/coverage-v8": "^4.0.8",
    "esbuild": "^0.25.12",
    "ink-testing-library": "^4.0.0",
    "patch-package": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.19.0 || >=20.6.0"
  }
}
